<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.plugin.pluginAdd" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>pluginAdd</name>
    <js-module name="pluginAdd" src="www/pluginAdd.js">
        <clobbers target="pluginAdd" /></js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
        <feature name="pluginAdd"><param name="android-package" value="com.plugin.pluginAdd.pluginAdd" /></feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/pluginAdd.java" target-dir="src/com/plugin/pluginAdd/pluginAdd" />
        <source-file src="src/android/FakeR.java" target-dir="src/com/plugin/pluginAdd/pluginAdd" />
    </platform>
</plugin>